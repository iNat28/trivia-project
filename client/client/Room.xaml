<local:LogoutWindow x:Class="client.Room"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:client"
        mc:Ignorable="d"
        Title="Room" Height="450" Width="800">
    <Grid>
        <Button x:Name="LeaveRoomButton" Content="Leave Room" HorizontalAlignment="Left" Margin="492,281,0,0" VerticalAlignment="Top" Width="75" Click="LeaveGameButton_Click"/>
        <Button x:Name="StartGameButton" Content="Start Game" HorizontalAlignment="Left" Margin="233,281,0,0" VerticalAlignment="Top" Width="75" Click="StartGameButton_Click"/>
        <Button x:Name="CloseRoomButton" Content="Close Room" HorizontalAlignment="Left" Margin="233,325,0,0" VerticalAlignment="Top" Width="75" Click="CloseRoomButton_Click"/>
        <ListBox x:Name="NamesList" HorizontalAlignment="Left" Height="135" Margin="303,109,0,0" VerticalAlignment="Top" Width="159" SelectionChanged="ListBox_SelectionChanged"/>
        <TextBlock x:Name="errorOutput" HorizontalAlignment="Left" Margin="25,35,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="310" Width="181"/>

    </Grid>
</local:LogoutWindow>
