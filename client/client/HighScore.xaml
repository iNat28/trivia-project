        <TextBlock x:Name="HighScoresHeader" FontSize="16" FontWeight="DemiBold" HorizontalAlignment="Center" Margin="236,30,424.6,0" TextWrapping="Wrap" Text=" High Scores:" VerticalAlignment="Top" Height="38" Width="133"/>
        <Button x:Name="BackButton" Content="Back" HorizontalAlignment="Left" Margin="347,337,0,0" VerticalAlignment="Top" Width="117" Height="42" Click="BackButton_Click"/>
