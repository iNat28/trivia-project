<src:LogoutWindow x:Class="client.MyStatus"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:src="clr-namespace:client"
        mc:Ignorable="d"
        Title="MyStatus" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="numOfGamesOutput" HorizontalAlignment="Left" Margin="296,171,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="147" Height="15"/>
        <TextBlock x:Name="numOfRightAnswersOutput" HorizontalAlignment="Left" Margin="296,207,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="16" Width="125"/>
        <TextBlock x:Name="numOfWrongAnswersOutput" HorizontalAlignment="Left" Margin="296,241,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="14" Width="127"/>
        <TextBlock x:Name="avgTimeForAnswersOutput" HorizontalAlignment="Left" Margin="296,271,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="16" Width="127"/>
        <Button x:Name="BackButton" Content="Back" HorizontalAlignment="Left" Margin="332,359,0,0" VerticalAlignment="Top" Width="75" Click="BackButton_Click"/>
        <TextBlock x:Name="errorOutput" HorizontalAlignment="Left" Margin="24,24,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="355" Width="272"/>
    </Grid>
</src:LogoutWindow>
