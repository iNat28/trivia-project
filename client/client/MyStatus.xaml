<local:LogoutWindow x:Class="client.MyStatusWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:client"
        mc:Ignorable="d"
        Title="My Status" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="numOfGamesOutput" HorizontalAlignment="Left" Margin="296,171,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="212" Height="15"/>
        <TextBlock x:Name="numOfRightAnswersOutput" HorizontalAlignment="Left" Margin="296,207,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="16" Width="210"/>
        <TextBlock x:Name="numOfWrongAnswersOutput" HorizontalAlignment="Left" Margin="296,241,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="14" Width="212"/>
        <TextBlock x:Name="avgTimeForAnswersOutput" HorizontalAlignment="Left" Margin="296,271,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="16" Width="212"/>
        <Button x:Name="BackButton" Content="Back" HorizontalAlignment="Left" Margin="332,359,0,0" VerticalAlignment="Top" Width="75" Click="BackButton_Click"/>
        <TextBlock x:Name="errorOutput" HorizontalAlignment="Left" Margin="24,24,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="355" Width="272"/>
        <TextBlock x:Name="numPointsOutput" HorizontalAlignment="Left" Margin="296,139,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="212" Height="15"/>
    </Grid>
</local:LogoutWindow>
